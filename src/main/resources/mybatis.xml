<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

    <settings>
        <setting name="jdbcTypeForNull" value="VARCHAR"/>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias type="java.lang.String" alias="string" />
        <typeAlias type="java.lang.Integer" alias="int" />
        <typeAlias type="java.util.Map" alias="map" />
        <typeAlias type="java.util.List" alias="list" />
        <typeAlias type="java.util.HashMap" alias="hashMap" />

        <typeAlias type="com.ygo.server.api.service.vo.UserVO" alias="user" />
        <typeAlias type="com.ygo.server.api.service.vo.FighterVO" alias="fighter" />
        <typeAlias type="com.ygo.server.api.service.vo.DeckVO" alias="deck" />
    </typeAliases>

</configuration>
